<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.framework.po.Views" table="tb_views">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id>
		<property name="ip" type="string" column="ip"/>
		<property name="macAddress" type="string" column="macAddress"/>
		<property name="type" type="string" column="type"/>
		<property name="createTime" type="java.sql.Timestamp" column="createTime"/>
		<property name="address" type="string" column="address"/>
		<property name="content" type="string" column="content" />
		<property name="flag" type="java.lang.Byte" column="flag"/>
	</class>
</hibernate-mapping>