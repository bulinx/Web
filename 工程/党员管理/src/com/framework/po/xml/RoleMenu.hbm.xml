<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.framework.po.RoleMenu" table="tb_role_menu">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id>
		<property name="roleId" type="java.lang.Integer" column="roleId"/>
		<property name="menuId" type="java.lang.Integer" column="menuId"/>
		<property name="state" type="java.lang.Byte" column="state" />
		<property name="flag" type="java.lang.Byte" column="flag"/>
	</class>
</hibernate-mapping>