<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.framework.po.Button" table="tb_button">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id> 
		<property name="name" type="string" column="name"/>
		<property name="subName" type="string" column="subName"/>
		<property name="subId" type="java.lang.Integer" column="subId"/>
		<property name="identity" type="string" column="identity"/>
		<property name="content" type="string" column="content"/>
		<property name="state" type="java.lang.Byte" column="state"/>
		<property name="flag" type="java.lang.Byte" column="flag"/>
	</class>
</hibernate-mapping>