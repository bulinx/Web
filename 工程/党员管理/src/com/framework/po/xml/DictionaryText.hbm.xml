<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.framework.po.DictionaryText" table="tb_dictionary_text">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id> 
		<property name="name" type="string" column="name"/>
		<property name="value" type="string" column="value"/>
		<property name="dicId" type="java.lang.Integer" column="dicId"/>
		<property name="dicName" type="string" column="dicName"/>
		<property name="state" type="java.lang.Byte" column="state"/>
		<property name="flag" type="java.lang.Byte" column="flag"/>
	</class>
</hibernate-mapping>