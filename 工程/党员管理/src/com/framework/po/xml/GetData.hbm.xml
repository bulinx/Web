<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.framework.duoshuo.GetData" table="tb_duoshuo">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id>
		<property name="flag" type="java.lang.Byte" column="flag"/> 
		<property name="code" type="java.lang.Integer" column="code"/>
		<property name="errorMessage" type="string" column="errorMessage"/>
		<property name="last_log_id" type="long" column="last_log_id"/>
		<property name="createTime" type="java.sql.Timestamp" column="createTime"/>
		<property name="action" type="string" column="action"/>
		<property name="signature" type="string" column="signature"/>
	</class>
</hibernate-mapping>