<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xgdj.po.Process" table="tb_xg_process">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"></generator>
		</id> 
		<property name="title" type="string" column="title"/><!--标题-->
		<property name="content" type="string" column="content"/><!--内容-->
		<property name="state" type="java.lang.Integer" column="state"/><!--代码值-->
		<property name="createTime" type="java.sql.Timestamp" column="createTime"/><!--操作时间-->
	</class>
</hibernate-mapping>